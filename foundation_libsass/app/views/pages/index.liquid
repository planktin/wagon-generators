---
title: Home
listed: true
published: true
cache_strategy: none
response_type: text/html
position: 0
---
<!DOCTYPE html>
<!--[if IE 9]><html class="lt-ie10" lang="en" > <![endif]-->
<html class="no-js" lang="en" >
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>
        {{ site.name }}: {{ page.title }}
        |
        {% if page.seo_title %}{{ page.seo_title }}{% else %}{{ site.seo_title }}{% endif %}
    </title>

    <meta name="description" content="{% if page.meta_description %}{{ page.meta_description }}{% else %}{{ site.meta_description }}{% endif %}">
    <meta name="keywords" content="{{ site.meta_keywords }}, {{ page.meta_keywords }}">

    <link rel="canonical" href="{{ canonical_url }}">

    {{ 'app.css' | stylesheet_tag }}
    {{ 'custom.modernizr.js' | javascript_tag }}
</head>
<body id="{{ page.slug }}">

    <!-- Header here -->
    {% include 'header' %}

    <!-- Content here -->
    {% block main %}
    <div class="row">
        <div class="large-12 columns">
            <h2>{{ page.title }}</h2>
            <p>An empty template... We&rsquo;ve got some work to do...</p>
        </div>
    </div>
    {% endblock %}

    <!-- Footer here -->
    {% include 'footer' %}

    <!-- Scripts here -->
    {{ 'jquery.js' | javascript_tag }}
    {{ 'foundation.min.js' | javascript_tag }}
    <script> $(document).foundation(); </script>
</body>
</html>
